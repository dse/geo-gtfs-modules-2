#!/usr/bin/env perl
use warnings;
use strict;

use Carp qw();
BEGIN {
    if ($ENV{DEBUG}) {
	$SIG{__DIE__} = \&Carp::confess;
    }
}

BEGIN {
    require lib;
    if (-d "/home/dse") {
        import lib "/home/dse/git/geo-gtfs-modules-2/Geo-GTFS2/lib";
        import lib "/home/dse/git/perl-app-thingy/lib";
    } elsif (-d "/Users/dse") {
        import lib "/Users/dse/git/geo-gtfs-modules-2/Geo-GTFS2/lib";
        import lib "/Users/dse/git/perl-app-thingy/lib";
    }
}

use FindBin;
use lib "$FindBin::Bin/../lib";

use Geo::GTFS2::CommandLine;

my $cl = Geo::GTFS2::CommandLine->new();
$cl->run(@ARGV);
